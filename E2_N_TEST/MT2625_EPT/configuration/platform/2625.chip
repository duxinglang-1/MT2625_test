[Chip Type]
Chip = 2625
GPIO_ModeNum = 9
;GPIO_Pull_Sel = 1

; 1-support 0-no support
Eint_Support = 1

; Keypad_Support 0 -- No support
; Keypad_Support 1 -- Support single  keypad (Extend_Key_Config = 0)
; Keypad_Support 2 -- Support double  keypad (Extend_Key_Config = 1, but no-triple)
; Keypad_Support 3 -- Support tripple keypad (Extend_Key_Config = 1)
Keypad_Support = 2

; GPIO_PUPD_Support 1 = MT2625 PUPD+PUPD2 (GPIO_PUPD_2625)
; GPIO_PUPD_Support 2 = PD7687 (GPIO_PUPD_7687)
GPIO_PUPD_Support = 1

; GPIO support: change variable name
GPIO_Var_Support = 1

; GPIO support: change pull-up/down when mode is peripheral mode.
GPIO_PUPD_Peripheral_Support = 1

; GPIO support: gen GPIO use_mode_array code
GPIO_Gen_USEMODE_Support = 0

; GPIO voltage support: gen GPIO with voltage information. 1,enable. 0,disable
